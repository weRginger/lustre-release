mostlyclean-libtool:
	-rm -f *.lo

clean-libtool:
	-rm -rf .libs _libs
install-modulefsDATA: $(modulefs_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(modulefsdir)" || $(MKDIR_P) "$(DESTDIR)$(modulefsdir)"
	@list='$(modulefs_DATA)'; test -n "$(modulefsdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(modulefsdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(modulefsdir)" || exit $$?; \
	done

uninstall-modulefsDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(modulefs_DATA)'; test -n "$(modulefsdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(modulefsdir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(modulefsdir)" && rm -f $$files
